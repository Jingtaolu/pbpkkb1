<p id="notice"><%= notice %></p>

<h1>Listing Pbpkmodels</h1>

<table>
  <thead>
    <tr>
      <th>Chemicalname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pbpkmodels.each do |pbpkmodel| %>
      <tr>
        <td><%= pbpkmodel.chemicalname %></td>
        <td><%= link_to 'Show', pbpkmodel %></td>
        <td><%= link_to 'Edit', edit_pbpkmodel_path(pbpkmodel) %></td>
        <td><%= link_to 'Destroy', pbpkmodel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pbpkmodel', new_pbpkmodel_path %>
